 Reordered Power of 2



class Solution {
public:
    bool reorderedPowerOf2(int n) {
     int x=1;
     string t=to_string(n);
     sort(t.begin(),t.end());
     for(int i=0;i<32;i++){
        string s=to_string(x);
        sort(s.begin(),s.end());
        if(s==t) return true;
        x<<=1;
     }  
     return false;
    }
};
